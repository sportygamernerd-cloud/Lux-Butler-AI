generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Property {
  id                  String   @id @default(uuid())
  name                String
  airbnb_link         String?
  wifi_ssid           String?
  wifi_password       String?
  instructions_entree String?
  secrets_maison      String?
  createdAt           DateTime @default(now())
}
